{
  "name": "convex-tanstack",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "sentry-build": "vite build && cp instrument.server.mjs .output/server",
    "sentry-start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "cf-deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "postinstall": "pnpm cf-typegen"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.13.7",
    "@convex-dev/react-query": "0.0.0-alpha.11",
